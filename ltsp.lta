(lt64-asm-prog
  (static
    (:str prompt ">> "))
    
  (main
    :label main-loop
    :push prompt :prnmem-lb
    :push read/read-form :call
    :push eval/eval-form :call
    :push print/print-form :call
    :push main-loop :jump

    :halt)
  
  (include "read.lta")
  (include "eval.lta")
  (include "print.lta")
  )
